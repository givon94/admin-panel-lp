mixin footer-contacts
    #footerContactsModal.footer_contacts-content(itemscope itemtype='http://schema.org/Organization')
        button#footerContactsClose.footer_contacts-close
        img.main-logo.contacts-logo(src='', alt='логотип компании')

        .footer_contacts-item(itemprop='name') Тест
        //-div(itemprop='address' itemscope itemtype='http://schema.org/PostalAddress')
            span
                b Адрес: &#32;
            span(itemprop='addressLocality') г. Минск
            span ,
            br
            span(itemprop='streetAddress') ул.
            br
            span
                b Индекс: &#32;
            span(itemprop='postalCode') 220000

        .footer_contacts-item.footer_contacts-item-phones
            b Телефоны: &#32;



        //-.footer_contacts-item
            b Страницы:
            //-ul.footer_contacts-item-lists(itemscope='', itemtype='http://schema.org/BreadcrumbList')

                li.footer_contacts-item-list(itemscope='', itemprop='itemListElement', itemtype='http://schema.org/ListItem')
                    a(rel='nofollow', itemprop='item', title='Заголовок', href='index.html' target="_blank")
                        span(itemprop='name') Заголовок
                        meta(itemprop='position', content='1')

                li.footer_contacts-item-list(itemscope='', itemprop='itemListElement', itemtype='http://schema.org/ListItem')
                    a(rel='nofollow', itemprop='item', title='Заголовок', href='index.html' target="_blank")
                        span(itemprop='name') Заголовок
                        meta(itemprop='position', content='2')

